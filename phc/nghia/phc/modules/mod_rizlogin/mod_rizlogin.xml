<?xml version="1.0" encoding="utf-8"?><install type="module" version="1.5.0">    <name>RizVN Login</name>	<author>Phuoc Nguyen</author>	<creationDate>June 2011</creationDate>	<copyright>Copyright (C) 2005 - 2010 Open Source Matters. All rights reserved.</copyright>	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>	<authorEmail>rizvn93@gmail.com</authorEmail>	<authorUrl>www.rizvn.net</authorUrl>	<version>1.1</version>	<description>RizVN Login - www.rizvn.net - Support 24 Language</description>
	<files>		<filename module="mod_rizlogin">mod_rizlogin.php</filename>        <filename>mod_rizlogin.xml</filename>		<filename>index.html</filename>		<filename>helper.php</filename>		<filename>tmpl/default.php</filename>		<filename>tmpl/index.html</filename>		<filename>images/background.gif</filename>		<filename>images/slide-button.gif</filename>		<filename>images/slide-button-active.gif</filename>		<filename>images/slide-button-o.gif</filename>		<filename>images/slide-button-o-active.gif</filename>		<filename>images/index.html</filename>		<filename>js/jquery.min.js</filename>		<filename>js/jquery-ui.min.js</filename>		<filename>js/side-bar.js</filename>		<filename>js/index.html</filename>		<filename>css/style.css</filename>		<filename>css/index.html</filename>			<folder>languages</folder>		</files>			<languages folder="languages">		<language tag="cs-CZ">cs-CZ.mod_rizlogin.ini</language>		<language tag="de-DE">de-DE.mod_rizlogin.ini</language>		<language tag="en-AU">en-AU.mod_rizlogin.ini</language>		<language tag="en-GB">en-GB.mod_rizlogin.ini</language>		<language tag="es-ES">es-ES.mod_rizlogin.ini</language>		<language tag="fr-FR">fr-FR.mod_rizlogin.ini</language>		<language tag="hr-HR">hr-HR.mod_rizlogin.ini</language>		<language tag="hu-HU">hu-HU.mod_rizlogin.ini</language>		<language tag="it-IT">it-IT.mod_rizlogin.ini</language>		<language tag="ja-JP">ja-JP.mod_rizlogin.ini</language>		<language tag="lt-LT">lt-LT.mod_rizlogin.ini</language>		<language tag="mk-MK">mk-MK.mod_rizlogin.ini</language>		<language tag="pt-PT">pt-PT.mod_rizlogin.ini</language>		<language tag="ro-RO">ro-RO.mod_rizlogin.ini</language>		<language tag="ru-RU">ru-RU.mod_rizlogin.ini</language>		<language tag="sk-SK">sk-SK.mod_rizlogin.ini</language>		<language tag="sr-RS">sr-RS.mod_rizlogin.ini</language>		<language tag="sv-SE">sv-SE.mod_rizlogin.ini</language>		<language tag="th-TH">th-TH.mod_rizlogin.ini</language>		<language tag="tr-TR">tr-TR.mod_rizlogin.ini</language>		<language tag="uk-UA">uk-UA.mod_rizlogin.ini</language>		<language tag="vi-VN">vi-VN.mod_rizlogin.ini</language>		<language tag="zh-CN">zh-CN.mod_rizlogin.ini</language>		<language tag="zh-TW">zh-TW.mod_rizlogin.ini</language>	</languages>		<params>		<param name="cache" type="list" default="1" label="Caching" description="Select whether to cache the content of this module">			<option value="0">Never</option>		</param>		<param name="@spacer" type="spacer" default="" label="" description="" />		<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="PARAMMODULECLASSSUFFIX" />		<param name="pretext" type="textarea" cols="30" rows="5" default="" label="Pre-text" description="PARAMPRETEXT" />		<param name="posttext" type="textarea" cols="30" rows="5" label="Post-text" description="PARAMPOSTTEXT" />		<param name="login" type="menuitem" default="" disable="separator" label="Login Redirection URL" description="PARAMLOGINREDIRECTURL" state="1" />		<param name="logout" type="menuitem" default="" disable="separator" label="Logout Redirection URL" description="PARAMLOGOUTREDIRECTURL" state="1" />		<param name="greeting" type="radio" default="1" label="Greeting" description="Show/Hide the simple greeting text">			<option value="0">No</option>			<option value="1">Yes</option>		</param>		<param name="name" type="list" default="0" label="Name/Username">			<option value="0">Username</option>			<option value="1">Name</option>		</param>		<param name="usesecure" type="radio" default="0" label="Encrypt Login Form" description="Submit encrypted login data (requires SSL)">			<option value="0">No</option>			<option value="1">Yes</option>		</param>	</params>
</install>